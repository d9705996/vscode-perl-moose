{
  "name": "perl-moose",
  "displayName": "Perl Moose",
  "description": "Perl Moose support for Visual Studio Code",
  "version": "1.0.0-rc2",
  "publisher": "torrentalle",
  "repository": {
    "type": "git",
    "url": "https://github.com/torrentalle/vscode-perl-moose"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "images/moose.png",
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.perl"
        ],
        "scopeName": "source.perl.moose",
        "name": "Perl Moose",
        "path": "./syntaxes/perl-moose.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^8.0.0",
    "js-yaml": "^3.12.1",
    "typescript": "^2.7.2",
    "vscode": "^1.1.21"
  },
  "scripts": {
    "version": "echo $npm_package_version",
    "build": "tsc -p tsconfig.json",
    "generate:syntax": "js-yaml src/syntaxes/perl-moose.tmLanguage.yaml > syntaxes/perl-moose.tmLanguage.json",
    "prepare": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "package": "vsce package"
  }
}
